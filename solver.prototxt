net: "network.prototxt"

test_interval: 50
test_iter: 1
display: 1000

# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.3
momentum: 0.3
lr_policy: "fixed"

# The maximum number of iterations
max_iter: 5000

# snapshot intermediate results
snapshot_prefix: "faces"

solver_mode: CPU

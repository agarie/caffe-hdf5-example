net: "faces_train_test.prototxt"

test_iter: 10
test_interval: 10
display: 10

# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.01

lr_policy: "fixed"

# The maximum number of iterations
max_iter: 500

# snapshot intermediate results
snapshot: 1000
snapshot_prefix: "faces"

solver_mode: CPU
